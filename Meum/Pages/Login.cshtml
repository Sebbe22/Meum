@page
@model Meum.Pages.LoginModel
@{
    ViewData["Title"] = "Login";
    
}
<style>

    .center {
        text-align: center;
        margin: auto;
        width: 50%;
        height: 50%;
        padding: 15px;
        
    }
    
        header ul{
            display: none;
        }
    
</style>



<div class="center">
    <img src="/Images/Meumlogo.jpg" height="100" />
</div>
<div class="container">
    <div class="center" style="border: 20px #B8926B; background: #F5ECDB;">
    
        <form method="post">
            <div class="form-group">
                <label asp-for="UserName" class="col-form-label "></label>
                <div>
                    <input asp-for="UserName"/>
                </div>
            </div>
            <div class="form-group">
                

                    <label asp-for="Password" class="col-form-label">
                    </label>
               
                    <div>
                        <input asp-for="Password" />
                    </div>
</div>
            <div class="form-group">
                <button class="btn btn-outline-black">Log in</button>
            </div>
        </form>
    </div>
</div>